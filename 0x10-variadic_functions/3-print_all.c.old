#include "variadic_functions.h"
#include <stdio.h>
#include <stdarg.h>
/**
* print_all - function
* Description: its name
* @format: number
* Return: 0
*/
void print_all(const char * const format, ...)
{
	va_list p;
	int i = 0, j = 0;
	char *s;
	type typ[] = {
                {"c", "char"},
                {"i", "int"},
                {"f", "float"},
                {"s", "char*"}
        };

	va_start(p, format);
	while (format[i])
	{
		while (j < 4)
		{
		if (format[i] == typ[j].a)
		{
			s = va_arg(p, typ[j].b);
			if (s != NULL)
				printf("%format[i]", s);
			else
				printf("(nil)");

			//if (i != n - 1)
			//	printf("%s", separator);
			j = 0;
		}
		j++;
		i++;
	}
	printf("\n");
	va_end(p);
}

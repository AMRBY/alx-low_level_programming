#include <calc.h>
/**
 * get_op_func - Struct op
 * Description: get operation function
 * @s: The operator
 * Return: int
 */
int (*get_op_func(char *s))(int, int)
{
	op_t ops[] = {
        	{"+", op_add},
        	{"-", op_sub},
        	{"*", op_mul},
        	{"/", op_div},
        	{"%", op_mod},
        	{NULL, NULL}
	};
	int i = 0;

	while (i < 6 && ops[i].a != s)
		i++;
		return ops[i].b

	return ops[i].b
}
